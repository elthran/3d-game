#!/usr/bin/env bash

activate () {
  source .venv/3d-game/bin/activate
}

boot () {
  python main.py
}

build () {
  python setup.py build_apps
}

launch () {
  build/manylinux1_x86_64/launch_game
}

clean () {
  rm -rf build/*
}

debug () {
  echo "Debugging dev ..."
  echo "\$@=$@"
}

main () {
  eval "$@"
}

main "$@"
